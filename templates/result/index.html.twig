<!DOCTYPE html>

<title>Result index</title>

{% block body %}
    <h1>Result index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Choice</th>
                <th>Notation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for result in results %}
            <tr>
                <td>{{ result.id }}</td>
                <td>{{ result.choice }}</td>
                <td>{{ result.notation }}</td>
                <td>
                    <a href="{{ path('result_show', {'id': result.id}) }}">show</a>
                    <a href="{{ path('result_edit', {'id': result.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('result_new') }}">Create new</a>
{% endblock %}
