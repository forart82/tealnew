<!DOCTYPE html>

<title>Images</title>

{% block body %}
    <h1>Images</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ image.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ image.name }}</td>
            </tr>
            <tr>
                <th>Svg</th>
                <td>{{ image.svg }}</td>
            </tr>
            <tr>
                <th>SvgColor</th>
                <td>{{ image.svgColor }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ image.category }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('images_index') }}">back to list</a>

    <a href="{{ path('images_edit', {'id': image.id}) }}">edit</a>

    {{ include('images/_delete_form.html.twig') }}
{% endblock %}
